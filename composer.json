{
    "name": "joseki/pdf-response",
    "description": "Pdf response extension for Nette Framework",
    "authors": [
        {
            "name": "Miroslav Paulík",
            "email": "miras.paulik@seznam.cz"
        },
        {
            "name": "Jan Kuchař"
        },
        {
            "name": "Tomáš Votruba",
            "email": "tomas.vot@gmail.com"
        },
        {
            "name": "Jan Galek",
            "email": "jan.galek@troia-studio.cz"
        }
    ],
    "replace": {
        "castamir/pdf-response": "2.*"
    },
    "require": {
        "mpdf/mpdf": "7.*"
    },
    "require-dev": {
        "nette/application": "^2.4",
        "nette/tester": "^1.7",
        "symfony/dom-crawler": "~2.5",
        "symfony/css-selector": "~2.5",
        "latte/latte": "^2.4"
    },
    "conflict": {
        "mpdf/mpdf": "<=5.7.99"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "suggest": {
        "symfony/dom-crawler": "Allows filtering html tags.",
        "nette/nette": "PHP framework to which this extension belongs to."
    },
    "license": "LGPL-3.0",
    "autoload": {
        "classmap": ["src"]
    }
}
